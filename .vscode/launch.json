{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch (Debug)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Sandbox/Binaries/Sandbox.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "Build (Debug)",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "C:/Programs/MinGW64/bin/gdb.exe",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "Launch (Release)",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Sandbox/Binaries/Sandbox.exe",
			"args": [],
			"stopAtEntry": false,
			"preLaunchTask": "Build (Release)",
			"cwd": "${workspaceFolder}",
			"environment": [],
			"console": "externalTerminal"
		},
		
	],
	"compounds": []
}